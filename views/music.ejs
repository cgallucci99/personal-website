						<ol class="carousel-indicators">
							<li data-target="#video-carousel" data-slide-to="0" class="active"></li>
							<li data-target="#video-carousel" data-slide-to="1"></li>
							<li data-target="#video-carousel" data-slide-to="2"></li>
						</ol>
						<!-- Wrapper for slides -->
						<div class="carousel-inner" role="listbox" style="border-radius: 10px;">
							<div class="item active">
								<div class="embed-responsive embed-responsive-16by9">
									<iframe class="embed-responsive-item" width="560" height="315" src="https://www.youtube.com/embed/bWQX_B9zcd4?rel=0&amp;controls=0" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
								</div>
							</div>
							<div class="item">
								<div class="embed-responsive embed-responsive-16by9">
									<iframe class="embed-responsive-item" width="560" height="315" src="https://www.youtube.com/embed/bbw2HPnNEwU?rel=0&amp;controls=0" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
								</div>
							</div>
							<div class="item">
								<div class="embed-responsive embed-responsive-16by9">
									<iframe class="embed-responsive-item" width="560" height="315" src="https://www.youtube.com/embed/7f5SdSkCAI8?rel=0&amp;controls=0" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
								</div>
							</div>
						</div>
					</div>
					<div class="carousel-buttons">
						<button class="btn btn-default" href="#video-carousel" role="button" data-slide="prev">
							<i class="fas fa-arrow-left" style="color:#777;"></i>
							<span class="sr-only">Previous</span>
						</button>
						<span style="width:50px">|</span>
						<button class="btn btn-default" href="#video-carousel" role="button" data-slide="next">
							<i class="fas fa-arrow-right" style="color:#777;"></i>
							<span class="sr-only">Next</span>
						</button class="btn btn-default">
					</div>
				</div>
